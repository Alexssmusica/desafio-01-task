{
  "name": "desafio-01-task",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "importCsc:dev": "tsx src/importCvc.ts",
    "build": "tsup",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:carga": "npx autocannon -c 500 -d 30 --workers 10 --renderStatusCodes --latency --warmup [-c 1 -d 2] localhost:3333/tasks",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "type": "tsc --project tsconfig.json --noEmit",
    "prettier": "prettier src/**/*.{js,ts} --write"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "csv-parse": "^5.3.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.41",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/coverage-c8": "^0.29.7",
    "@vitest/ui": "^0.29.7",
    "autocannon": "7.10.0",
    "cors": "^2.8.5",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.5",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  }
}
